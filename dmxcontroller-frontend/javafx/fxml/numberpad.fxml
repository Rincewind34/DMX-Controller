<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root styleClass="basic-container" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
      <VBox styleClass="numpad">
         <children>
      		<Label fx:id="display" styleClass="basic-label">
      			<styleClass>
      				<String fx:value="basic-label" />
      				<String fx:value="basic-element" />
      			</styleClass>
      		</Label>
      		<GridPane>
      			<columnConstraints>
      				<ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
      				<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0" />
      				<ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
      				<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0" />
      				<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
      			</columnConstraints>
      			<rowConstraints>
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
      				<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
      			</rowConstraints>
      			<children>
      				<Button fx:id="button1" mnemonicParsing="false" text="1">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button2" mnemonicParsing="false" text="2" GridPane.columnIndex="2">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button3" mnemonicParsing="false" text="3" GridPane.columnIndex="4">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button4" mnemonicParsing="false" text="4" GridPane.rowIndex="2">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button5" mnemonicParsing="false" text="5" GridPane.columnIndex="2" GridPane.rowIndex="2">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button6" mnemonicParsing="false" text="6" GridPane.columnIndex="4" GridPane.rowIndex="2">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button7" mnemonicParsing="false" text="7" GridPane.rowIndex="4">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button8" mnemonicParsing="false" text="8" GridPane.columnIndex="2" GridPane.rowIndex="4">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button9" mnemonicParsing="false" text="9" GridPane.columnIndex="4" GridPane.rowIndex="4">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="buttonPlus" mnemonicParsing="false" text="+" GridPane.rowIndex="6">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="button0" mnemonicParsing="false" text="0" GridPane.columnIndex="2" GridPane.rowIndex="6">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="buttonThru" mnemonicParsing="false" text="Thru" GridPane.columnIndex="4" GridPane.rowIndex="6">
      					<styleClass>
      						<String fx:value="thru-button" />
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      			</children>
      			<styleClass>
      				<String fx:value="grid" />
      			</styleClass>
      		</GridPane>
      		<HBox prefHeight="0.0" prefWidth="0.0">
      			<children>
      				<Button fx:id="buttonClear" mnemonicParsing="false" text="Clear">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      				<Button fx:id="buttonBack" mnemonicParsing="false" text="Back">
      					<styleClass>
      						<String fx:value="basic-button" />
      						<String fx:value="basic-element" />
      					</styleClass>
      				</Button>
      			</children>
      			<styleClass>
      				<String fx:value="clear-button-container" />
      			</styleClass>
      		</HBox>
         </children>
      </VBox>
      <VBox alignment="CENTER" styleClass="fader-box">
         <children>
            <Slider fx:id="fader" orientation="VERTICAL" VBox.vgrow="ALWAYS">
               <styleClass>
                  <String fx:value="basic-slider" />
                  <String fx:value="basic-element" />
               </styleClass>
            </Slider>
            <Button fx:id="flashButton" mnemonicParsing="false" text="Flash">
               <styleClass>
                  <String fx:value="basic-button" />
                  <String fx:value="basic-element" />
               </styleClass>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
            <ToggleButton fx:id="settingButton" mnemonicParsing="false" text="Push 0">
               <styleClass>
                  <String fx:value="basic-toggle-button" />
                  <String fx:value="basic-element" />
               </styleClass>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </ToggleButton>
         </children>
      </VBox>
	</children>
   <stylesheets>
      <URL value="@../css/basics.css" />
      <URL value="@../css/numberpad.css" />
      <URL value="@../css/fader.css" />
   </stylesheets>
</fx:root>
