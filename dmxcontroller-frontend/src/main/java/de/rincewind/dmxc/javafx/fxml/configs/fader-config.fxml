<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" styleClass="basic-container" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TextField promptText="Caption" fx:id="textCaption">
			<styleClass>
				<String fx:value="basic-element" />
				<String fx:value="basic-text-field" />
			</styleClass>
		</TextField>
		<Region prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
		<Button mnemonicParsing="false" text="Set Selection" fx:id="buttonSetSelection">
			<styleClass>
				<String fx:value="basic-element" />
				<String fx:value="basic-button" />
			</styleClass>
		</Button>
	</children>
	<stylesheets>
		<URL value="@../../css/basics.css" />
		<URL value="@../../css/configs/fader-config.css" />
	</stylesheets>
</fx:root>
