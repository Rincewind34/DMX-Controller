<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root styleClass="template" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox fx:id="root" VBox.vgrow="ALWAYS">
			<children>
				<TabPane fx:id="selectorTabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" HBox.hgrow="ALWAYS">
					<tabs>
						<Tab text="Channels">
							<content>
								<VBox alignment="CENTER">
									<children>
										<Label fx:id="display">
											<styleClass>
												<String fx:value="basic-label" />
												<String fx:value="basic-element" />
											</styleClass>
										</Label>
										<GridPane styleClass="grid">
											<columnConstraints>
												<ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
												<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0" />
												<ColumnConstraints hgrow="ALWAYS" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
												<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="10.0" />
												<ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="30.0" />
											</columnConstraints>
											<rowConstraints>
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="10.0" vgrow="SOMETIMES" />
												<RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" vgrow="ALWAYS" />
											</rowConstraints>
											<children>
												<Button fx:id="button1" mnemonicParsing="false" text="1">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button2" mnemonicParsing="false" text="2" GridPane.columnIndex="2">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button3" mnemonicParsing="false" text="3" GridPane.columnIndex="4">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button4" mnemonicParsing="false" text="4" GridPane.rowIndex="2">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button5" mnemonicParsing="false" text="5" GridPane.columnIndex="2" GridPane.rowIndex="2">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button6" mnemonicParsing="false" text="6" GridPane.columnIndex="4" GridPane.rowIndex="2">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button7" mnemonicParsing="false" text="7" GridPane.rowIndex="4">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button8" mnemonicParsing="false" text="8" GridPane.columnIndex="2" GridPane.rowIndex="4">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button9" mnemonicParsing="false" text="9" GridPane.columnIndex="4" GridPane.rowIndex="4">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="buttonPlus" mnemonicParsing="false" text="+" GridPane.rowIndex="6">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="button0" mnemonicParsing="false" text="0" GridPane.columnIndex="2" GridPane.rowIndex="6">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="buttonThru" mnemonicParsing="false" text="Thru" GridPane.columnIndex="4" GridPane.rowIndex="6">
													<styleClass>
														<String fx:value="thru-button" />
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
											</children>
										</GridPane>
										<HBox prefHeight="0.0" prefWidth="0.0">
											<children>
												<Button fx:id="buttonClear" mnemonicParsing="false" text="Clear">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
												<Button fx:id="buttonBack" mnemonicParsing="false" text="Back">
													<styleClass>
														<String fx:value="basic-button" />
														<String fx:value="basic-element" />
													</styleClass>
												</Button>
											</children>
											<styleClass>
												<String fx:value="clear-button-container" />
											</styleClass>
										</HBox>
									</children>
									<styleClass>
										<String fx:value="numpad" />
										<String fx:value="tab-content" />
									</styleClass>
								</VBox>
							</content>
						</Tab>
						<Tab text="Submasters">
							<content>
								<VBox spacing="10.0" styleClass="tab-content">
									<children>
										<ListView fx:id="listSubmasters" VBox.vgrow="ALWAYS">
											<styleClass>
												<String fx:value="basic-element" />
												<String fx:value="basic-list" />
											</styleClass>
										</ListView>
										<Button fx:id="buttonRefreshSubmasters" mnemonicParsing="false" prefWidth="1000.0" text="Refresh">
											<styleClass>
												<String fx:value="basic-element" />
												<String fx:value="basic-button" />
											</styleClass>
										</Button>
									</children>
								</VBox>
							</content>
						</Tab>
						<Tab text="Effects">
							<content>
								<VBox spacing="10.0" styleClass="tab-content">
									<children>
										<ListView fx:id="listEffects" VBox.vgrow="ALWAYS">
											<styleClass>
												<String fx:value="basic-element" />
												<String fx:value="basic-list" />
											</styleClass>
										</ListView>
										<Button fx:id="buttonRefreshEffects" mnemonicParsing="false" prefWidth="1000.0" text="Refresh">
											<styleClass>
												<String fx:value="basic-element" />
												<String fx:value="basic-button" />
											</styleClass>
										</Button>
									</children>
								</VBox>
							</content>
						</Tab>
					</tabs>
				</TabPane>
				<Region prefHeight="200.0" prefWidth="200.0" styleClass="region" />
				<ScrollPane fx:id="contentScrollPane" hbarPolicy="ALWAYS" vbarPolicy="NEVER" HBox.hgrow="ALWAYS">
					<content>
						<HBox fx:id="content">
							<styleClass>
								<String fx:value="content" />
							</styleClass>
						</HBox>
					</content>
				</ScrollPane>
				<Region prefHeight="200.0" prefWidth="200.0" styleClass="region" />
				<ScrollPane fx:id="dragScrollPane" hbarPolicy="NEVER" prefHeight="100.0" prefWidth="0.0" styleClass="pane-selector" vbarPolicy="ALWAYS" HBox.hgrow="ALWAYS">
					<content>
						<VBox fx:id="dragContent" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="30.0" />
					</content>
				</ScrollPane>
			</children>
		</HBox>
		<HBox styleClass="buttonbox">
			<children>
				<Button fx:id="buttonConfig" mnemonicParsing="false" text="Configuration">
					<styleClass>
						<String fx:value="basic-element" />
						<String fx:value="basic-button" />
					</styleClass>
				</Button>
				<Region HBox.hgrow="ALWAYS" />
				<Button fx:id="buttonHome" mnemonicParsing="false" text="Home">
					<styleClass>
						<String fx:value="basic-element" />
						<String fx:value="basic-button" />
					</styleClass>
				</Button>
				<Region HBox.hgrow="ALWAYS" />
				<Button fx:id="buttonDrag" mnemonicParsing="false" text="Drag-Drop">
					<styleClass>
						<String fx:value="basic-element" />
						<String fx:value="basic-button" />
					</styleClass>
				</Button>
			</children>
		</HBox>
	</children>
	<stylesheets>
		<URL value="@../css/basics.css" />
		<URL value="@../css/template.css" />
		<URL value="@../css/numberpad.css" />
	</stylesheets>
</fx:root>
